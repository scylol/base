{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "js-cookie": "^2.1.3",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.0.2",
    "rc-slider": "^8.2.0",
    "rc-tooltip": "^3.4.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-fontawesome": "^1.6.1",
    "react-modal": "^2.2.2",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.2",
    "react-scripts": "^1.0.10",
    "redux": "^3.7.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "socket.io-client": "^2.0.3"
  },
  "scripts": {
    "build-css": "node-sass-chokidar  ./src/sass src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar  ./src/sass src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
